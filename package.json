{
  "name": "bar-lobby",
  "version": "0.14.0",
  "private": true,
  "description": "Lobby client for the RTS game Beyond All Reason",
  "author": "Jazcash <jaspercashmore@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyond-all-reason/bar-lobby.git"
  },
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "electron-vite",
    "debug": "electron-vite --remote-debugging-port=9223",
    "build": "electron-vite build",
    "release": "electron-builder --config .electron-builder.config.js",
    "local-build": "npm run build && npm run compile",
    "fix": "npm run type-check && npm run tidy",
    "check": "npm run type-check && npm run tidy-check",
    "type-check": "vue-tsc --noEmit -p tsconfig.node.json && vue-tsc --noEmit -p tsconfig.web.json",
    "tidy": "npm run lint && npm run format",
    "tidy-check": "npm run lint-check && npm run format-check",
    "lint": "eslint . --ext .ts,.vue --fix",
    "lint-check": "eslint . --ext .ts,.vue",
    "format": "prettier --write \"src/**/*.{ts,vue,scss,json}\"",
    "format-check": "prettier --check \"src/**/*.{ts,vue,scss,json}\"",
    "test": "vitest run",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "preversion": "npm run check",
    "postversion": "git push && git push --tags"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "build": {
    "electronDist": "node_modules/electron/dist"
  },
  "dependencies": {
    "@iconify-icons/mdi": "^1.2.14",
    "@iconify/json": "^2.2.14",
    "@iconify/vue": "^4.1.0",
    "@vueuse/core": "^10.0.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^0.24.0",
    "better-sqlite3": "^9.4.5",
    "date-fns": "^2.29.1",
    "dompurify": "^3.0.3",
    "env-paths": "^3.0.0",
    "flag-icons": "^6.1.1",
    "glob": "^10.3.10",
    "howler": "^2.2.3",
    "jaz-ts-utils": "^7.10.0",
    "kysely": "^0.21.6",
    "luaparse": "^0.3.1",
    "marked": "^5.0.2",
    "minimatch": "^9.0.3",
    "node-7z": "^3.0.0",
    "node-self": "^1.0.1",
    "octokit": "^2.0.14",
    "pixi.js": "^7.1.4",
    "primeicons": "^6.0.1",
    "primevue": "^3.23.0",
    "sdfz-demo-parser": "^5.9.1",
    "spring-map-parser": "^6.1.1",
    "steamworks.js": "^0.3.1",
    "tachyon-client": "^10.3.0",
    "vue-i18n": "^9.2.2",
    "vue3-markdown-it": "^1.0.9",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@sinclair/typebox": "^0.25.21",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/better-sqlite3": "^7.6.0",
    "@types/dompurify": "^3.0.2",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/howler": "^2.2.4",
    "@types/luaparse": "^0.2.8",
    "@types/marked": "^5.0.0",
    "@types/node": "^20.9.2",
    "@types/node-7z": "^2.1.5",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^7.5.1-alpha.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/compiler-sfc": "^3.4.21",
    "@vue/devtools": "^7.0.25",
    "bar-db": "^6.3.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.1",
    "electron": "^29.0.0",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "^5.3.0",
    "electron-vite": "^2.1.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "patch-package": "^8.0.0",
    "playwright": "^1.30.0",
    "prettier": "^2.6.2",
    "rimraf": "^4.1.2",
    "sass": "^1.52.1",
    "tsconfig-paths": "^4.1.2",
    "type-fest": "^4.15.0",
    "typescript": "^5.4.4",
    "unplugin-vue-router": "^0.5.4",
    "uuid": "^9.0.0",
    "vite": "^5.2.8",
    "vite-electron-plugin": "^0.8.3",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-esmodule": "^1.5.0",
    "vitest": "^0.28.4",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.11",
    "worker-plugin": "^5.0.1"
  },
  "overrides": {
    "eslint": "$eslint",
    "@typescript-eslint/eslint-plugin": "$@typescript-eslint/eslint-plugin",
    "eslint-plugin-unused-imports": "$eslint-plugin-unused-imports"
  }
}